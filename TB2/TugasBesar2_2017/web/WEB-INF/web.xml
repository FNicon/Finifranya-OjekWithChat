<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>HistoryHide</servlet-name>
        <servlet-class>web_service.hide_servlet.HistoryHide</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HistoryHide</servlet-name>
        <url-pattern>/HistoryHide</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>OrderDriverStatusChatting</servlet-name>
        <servlet-class>web_service.order_servlet.OrderDriverStatusChatting</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderDriverStatusChatting</servlet-name>
        <url-pattern>/OrderDriverStatusChatting</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>OrderDriverStatusFinding</servlet-name>
        <servlet-class>web_service.order_servlet.OrderDriverStatusFinding</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderDriverStatusFinding</servlet-name>
        <url-pattern>/OrderDriverStatusFinding</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>OrderDriverStatusPassive</servlet-name>
        <servlet-class>web_service.order_servlet.OrderDriverStatusPassive</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderDriverStatusPassive</servlet-name>
        <url-pattern>/OrderDriverStatusPassive</url-pattern>
    </servlet-mapping>
</web-app>